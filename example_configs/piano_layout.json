{
  "version": "1.0",
  "description": "Traditional piano keyboard layout - QWERTY row as white keys",
  "mappings": {
    "Q": {
      "note": "C4",
      "volume": 0.3,
      "description": "White key - Middle C"
    },
    "W": {
      "note": "D4",
      "volume": 0.3,
      "description": "White key - D"
    },
    "E": {
      "note": "E4",
      "volume": 0.3,
      "description": "White key - E"
    },
    "R": {
      "note": "F4",
      "volume": 0.3,
      "description": "White key - F"
    },
    "T": {
      "note": "G4",
      "volume": 0.3,
      "description": "White key - G"
    },
    "Y": {
      "note": "A4",
      "volume": 0.3,
      "description": "White key - A"
    },
    "U": {
      "note": "B4",
      "volume": 0.3,
      "description": "White key - B"
    },
    "I": {
      "note": "C5",
      "volume": 0.3,
      "description": "White key - High C"
    },
    "Key2": {
      "note": "C#4",
      "volume": 0.25,
      "description": "Black key - C sharp"
    },
    "Key3": {
      "note": "D#4",
      "volume": 0.25,
      "description": "Black key - D sharp"
    },
    "Key5": {
      "note": "F#4",
      "volume": 0.25,
      "description": "Black key - F sharp"
    },
    "Key6": {
      "note": "G#4",
      "volume": 0.25,
      "description": "Black key - G sharp"
    },
    "Key7": {
      "note": "A#4",
      "volume": 0.25,
      "description": "Black key - A sharp"
    },
    "Space": {
      "note": "C3",
      "volume": 0.1,
      "description": "Bass note for spacebar"
    },
    "Enter": {
      "note": "C2",
      "volume": 0.15,
      "description": "Deep bass for Enter"
    }
  }
}
